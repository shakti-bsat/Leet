class Solution:
    def merge(self, nums1: List[int], m: int, nums2: List[int], n: int) -> None:
        a,c = len(nums2), 0
        nums1.extend(nums2)
        for i in range(len(nums1)-1,-1,-1):
            if nums1[i] == 0:
                nums1.pop(i)
                c += 1 

        if abs(a-c) != 0:
            for i in range(abs(a-c)):
                nums1.append(0)
        print(nums1.sort())
